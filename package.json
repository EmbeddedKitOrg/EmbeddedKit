{
  "name": "embedkit-docs",
  "version": "1.0.0",
  "description": "EmbedKit documentation system",
  "main": "index.js",
  "scripts": {
    "docs:serve": "docsify serve docs",
    "docs:build": "node scripts/generate-sidebar.js && python scripts/update-docs-index.py",
    "docs:update": "npm run docs:build && npm run docs:serve",
    "sidebar:gen": "node scripts/generate-sidebar.js",
    "index:gen": "python scripts/update-docs-index.py",
    "api:gen": "python scripts/extract-api-docs.py"
  },
  "keywords": [
    "embedded",
    "documentation",
    "docsify"
  ],
  "author": "EmbedKit Team",
  "license": "MIT",
  "devDependencies": {
    "docsify-cli": "^4.4.4",
    "glob": "^8.0.3"
  },
  "dependencies": {}
}